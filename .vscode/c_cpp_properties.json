        {
            "name": "config",
            "configurationProvider": "ms-vscode.cmake-tools",
            "compilerPath": "${env:ONETHINX_COMPILER_LOC}/arm-none-eabi-gcc",
            "includePath": [
                "${workspaceFolder}/**",
                //"/Applications/gcc-arm-none-eabi-10.3-2021.10/arm-none-eabi/include/"
